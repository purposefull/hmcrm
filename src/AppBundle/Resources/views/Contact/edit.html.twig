{% extends '::dashboard.html.twig' %}

{% block title %} {{ 'lead.edit'|trans }} {% endblock %}

{% block body -%}
    {{ form_start(edit_form) }}
    <div class="form-actions">
        <div class="col-sm-12">
            {{ form_widget(edit_form.submit, { 'attr': {'class': 'btn btn-primary'}}) }}
